ceNode"><Name z:Ref="11" i:nil="true"/><OnEnter z:Ref="12" i:nil="true"/><Options z:Ref="13" i:nil="true"/><Text z:Ref="27" i:nil="true"/><Reference z:Ref="10" i:nil="true"/></Node><OnSelect z:Id="536" z:Size="0"/></Option><Option z:Id="537"><Conditionals z:Id="538" z:Size="1"><InventoryConditional z:Id="539"><ComparisonOperator>EqualTo</ComparisonOperator><InventoryItem z:Id="540">SleepingGuard</InventoryItem><Value>0</Value></InventoryConditional></Conditionals><Description z:Id="541">Head towards the door</Description><Node z:Id="542"><Name z:Id="543">Guard Room - First</Name><OnEnter z:Id="544" z:Size="0"/><Options z:Id="545" z:Size="2"><Option z:Id="546"><Conditionals z:Id="547" z:Size="0"/><Description z:Id="548">Open door slightly and take a peek...</Description><Node z:Id="549"><Name z:Id="550">Guard Room Peek</Name><OnEnter z:Id="551" z:Size="0"/><Options z:Id="552" z:Size="2"><Option z:Id="553"><Conditionals z:Id="554" z:Size="0"/><Description z:Id="555">Try to sneak past him</Description><Node z:Id="556"><Name z:Id="557">Guard Wakes Up</Name><OnEnter z:Id="558" z:Size="1"><InventoryOperation z:Id="559"><ItemName z:Id="560">SleepingGuard</ItemName><Operation>Set</Operation><Value>1</Value></InventoryOperation></OnEnter><Options z:Id="561" z:Size="1"><Option z:Id="562"><Conditionals z:Id="563" z:Size="0"/><Description z:Id="564">This door makes too much noise</Description><Node z:Id="565" i:type="ReferenceNode"><Name z:Ref="543" i:nil="true"/><OnEnter z:Ref="544" i:nil="true"/><Options z:Ref="545" i:nil="true"/><Text z:Id="566">As you aproach the door, you hear snoring coming from the other side.</Text><Reference z:Ref="542" i:nil="true"/></Node><OnSelect z:Id="567" z:Size="0"/></Option></Options><Text z:Id="568">The door creaks and the guard immidiately starts to stir. You freeze as he grumbles back into an endless snore.</Text></Node><OnSelect z:Id="569" z:Size="0"/></Option><Option z:Id="570"><Conditionals z:Id="571" z:Size="0"/><Description z:Id="572">I