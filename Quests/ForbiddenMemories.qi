mparisonOperator>EqualTo</ComparisonOperator><InventoryItem z:Id="401">Brick</InventoryItem><Value>2</Value></InventoryConditional></Conditionals><Description z:Id="402">Retrieve Brick</Description><Node z:Id="403"><Name z:Id="404">Walk Over To Brick</Name><OnEnter z:Id="405" z:Size="2"><InventoryOperation z:Id="406"><ItemName z:Id="407">Brick</ItemName><Operation>Set</Operation><Value>1</Value></InventoryOperation><InventoryOperation z:Id="408"><ItemName z:Id="409">Brick</ItemName><Operation>MakeVisible</Operation><Value>0</Value></InventoryOperation></OnEnter><Options z:Id="410" z:Size="1"><Option z:Id="411"><Conditionals z:Id="412" z:Size="0"/><Description z:Id="413">Cool. That was easy.</Description><Node z:Id="414" i:type="ReferenceNode"><Name z:Ref="11" i:nil="true"/><OnEnter z:Ref="12" i:nil="true"/><Options z:Ref="13" i:nil="true"/><Text z:Ref="27" i:nil="true"/><Reference z:Ref="10" i:nil="true"/></Node><OnSelect z:Id="415" z:Size="0"/></Option></Options><Text z:Id="416">You walk over to the brick and pick it up.</Text></Node><OnSelect z:Id="417" z:Size="0"/></Option><Option z:Id="418"><Conditionals z:Id="419" z:Size="1"><InventoryConditional z:Id="420"><ComparisonOperator>EqualTo</ComparisonOperator><InventoryItem z:Id="421">Spoon</InventoryItem><Value>0</Value></InventoryConditional></Conditionals><Description z:Id="422">Look Under Cot</Description><Node z:Id="423"><Name z:Id="424">Under Bed</Name><OnEnter z:Id="425" z:Size="2"><InventoryOperation z:Id="426"><ItemName z:Id="427">Spoon</ItemName><Operation>Set</Operation><Value>1</Value></InventoryOperation><InventoryOperation z:Id="428"><ItemName z:Id="429">Spoon</ItemName><Operation>MakeVisible</Operation><Value>0</Value></InventoryOperation></OnEnter><Options z:Id="430" z:Size="1"><Option z:Id="431"><Conditionals z:Id="432" z:Size="0"/><Description z:Id="433">Now, to find a way out...</Description><Node z:Id="434" i:type="ReferenceNode"><Name z:Ref="11" i:nil="true"/><OnEnter z:Ref="12" i:nil="true"/><O